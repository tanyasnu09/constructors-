public class Helo{
    Helo(String a, String b){
        System.out.println("My name is " + a + " and I live in " + b);
    }
    Helo(String a,String b, String c){
        System.out.println("My name is "+a + " and I live in "+ b + " pursuing "+ c);
    }

}

public class Main{
    public static void main (String[] args){
        Helo h= new Helo( "tanya", "Patna");
        Helo h2 =  new Helo( "tanya", "Patna", "EEE");

    }
}
